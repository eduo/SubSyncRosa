#tag ClassProtected Class subLine	#tag Method, Flags = &h0		Function lines(optional EOF as string) As string		  if EOF.len = 0 then EOF = EndOfLine		  		  Dim thisText As String		  		  If me.subText.Ubound > -1 Then		    For ii As Integer = 0 To me.subText.Ubound		      if ii > 0 then		        thisText =  thisText + EOF + me.subtext(ii)		      else		        thisText = me.subtext(ii)		      end if		    next		  end if		  		  return thisText		End Function	#tag EndMethod	#tag Property, Flags = &h0		durationSeconds As double	#tag EndProperty	#tag Property, Flags = &h0		endSeconds As double	#tag EndProperty	#tag Property, Flags = &h0		endTime As string	#tag EndProperty	#tag Property, Flags = &h0		length As Integer	#tag EndProperty	#tag Property, Flags = &h0		lineID As Integer	#tag EndProperty	#tag Property, Flags = &h0		startSeconds As double	#tag EndProperty	#tag Property, Flags = &h0		startTime As string	#tag EndProperty	#tag Property, Flags = &h0		subText() As string	#tag EndProperty	#tag ViewBehavior		#tag ViewProperty			Name="durationSeconds"			Group="Behavior"			Type="double"		#tag EndViewProperty		#tag ViewProperty			Name="endSeconds"			Group="Behavior"			Type="double"		#tag EndViewProperty		#tag ViewProperty			Name="endTime"			Group="Behavior"			Type="string"			EditorType="MultiLineEditor"		#tag EndViewProperty		#tag ViewProperty			Name="Index"			Visible=true			Group="ID"			InitialValue="-2147483648"			Type="Integer"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Left"			Visible=true			Group="Position"			InitialValue="0"			Type="Integer"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="length"			Group="Behavior"			Type="Integer"		#tag EndViewProperty		#tag ViewProperty			Name="lineID"			Group="Behavior"			Type="Integer"		#tag EndViewProperty		#tag ViewProperty			Name="Name"			Visible=true			Group="ID"			Type="St